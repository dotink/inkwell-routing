{
	"name": "dotink/inkwell-routing",
	"description": "Route incoming requests to various callbacks or directly to files",
	"keywords": ["router"],
	"license": "AGPL-3.0",
	"type": "opus-package",
	"authors": [
		{
			"name": "Matthew J. Sahagian",
			"email": "msahagian@dotink.org",
			"role": "Lead Architect / Developer"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.dotink.org"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"imarc/opus": "dev-master",
		"dotink/flourish-core": "dev-master",
		"dotink/inkwell-http": "dev-master",
		"dotink/inkwell-events": "dev-master"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"extra": {
		"opus": {
			"inkwell/framework": {
				"plugin/config" : "config",
				"plugin/include" : "include",
				"plugin/public" : "public"
			}
		}
	}
}
